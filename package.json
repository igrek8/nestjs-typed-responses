{
  "name": "nestjs-typed-responses",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Igor Korchagin (https://github.com/igrek8)",
  "keywords": [
    "api",
    "nest.js",
    "open-api"
  ],
  "description": "Enables OpenAPI v3 polymorphism using `__type` metafield to resolve different exceptions or responses within the same HTTP code response group",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/igrek8/nestjs-typed-responses#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/igrek8/nestjs-typed-responses"
  },
  "bugs": {
    "url": "https://github.com/igrek8/nestjs-typed-responses/issues"
  },
  "files": [
    "./dist"
  ],
  "prettier": "@igrek8/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@igrek8/semantic-release-config"
  },
  "jest": {
    "preset": "@igrek8/jest-preset"
  },
  "eslintConfig": {
    "extends": "@igrek8/eslint-config",
    "env": {
      "node": true
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "class-transformer": "^0",
    "class-validator": "^0",
    "reflect-metadata": "^0",
    "tslib": "^2"
  },
  "peerDependencies": {
    "@nestjs/common": "^9 || ^10",
    "@nestjs/core": "^9 || ^10",
    "@nestjs/platform-express": "^9 || ^10",
    "@nestjs/swagger": "^6 || ^7"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "compile": "tsc -p tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "lint": "eslint \"**/*\"",
    "format": "prettier --check \"**/*\"",
    "docs": "typedoc build -o docs && typedoc publish -o docs",
    "start": "ts-node ./src/demo/main.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@igrek8/eslint-config": "1.0.6",
    "@igrek8/jest-preset": "1.0.11",
    "@igrek8/prettier-config": "1.0.11",
    "@igrek8/semantic-release-config": "1.0.11",
    "@igrek8/tsconfig": "1.0.11",
    "@igrek8/typedoc": "1.1.2",
    "@nestjs/common": "10.2.10",
    "@nestjs/core": "10.3.0",
    "@nestjs/platform-express": "10.2.10",
    "@nestjs/swagger": "7.1.17",
    "@nestjs/testing": "10.3.0",
    "@types/express": "4.17.21",
    "@types/node": "20.10.5",
    "@types/supertest": "2.0.16",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "eslint": "8.56.0",
    "jest": "29.7.0",
    "rimraf": "5.0.5",
    "semantic-release": "20.1.3",
    "supertest": "6.3.3",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "typescript": "4.9.5",
    "utility-types": "3.10.0"
  }
}
